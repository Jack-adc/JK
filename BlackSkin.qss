/*************Qt样式表*************/

* {
    background-color: rgb(37, 37, 38);
    color: white;
    selection-background-color: rgb(0, 111, 185);
}

*::item:selected:!active {
    background-color: rgb(63, 63, 70);
    color: white;
}

QMainWindow, QToolButton, *[framecolor="true"] {
    background-color: rgb(45, 45, 48);
}

QMenuBar {
    background-color: rgb(37, 37, 38);
}

QGraphicsView {
	background-color: rgb(45, 45, 48);
	border: none;
}

QToolButton#colorEditToolButton {
    background-color: rgb(80, 80, 80);
    color: white;
}

QToolButton#colorEditToolButton:hover {
    background-color: rgb(70, 110, 150);
    color: white;
}

QToolButton#colorEditToolButton:pressed {
    background-color: rgb(0, 111, 185);
    color: white;
}

QPushButton {
    background-color: rgb(80, 80, 80);
    color: white;
    border: 0px solid rgb(62, 62, 66);
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QPushButton:disabled {
	background-color: rgb(165, 165, 165);
    color: white;
    border: 0px solid rgb(62, 62, 66);
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QPushButton:default {
    background-color: rgb(70, 110, 150);
    color: white;
    border: 0px solid rgb(62, 62, 66);
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QPushButton:hover {
    background-color: transparent;
    color: white;
    border: 1px solid rgb(62, 62, 66);
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 3px;
    padding-bottom: 3px;
}

QPushButton:pressed {
    background-color: rgb(0, 111, 185);
    color: white;
    border: 0px solid rgb(62, 62, 66);
    padding-left: 8px;
    padding-right: 8px;
    padding-top: 3px;
    padding-bottom: 3px;
}
QPushButton:checked {
    background-color:rgb(14, 170, 79);
    border-radius: 10px;
    color: rgb(255, 255, 0);
}

QPushButton#btnPswShow {
    background-color: transparent;  
	border: 0px solid rgb(62, 62, 66);
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
}

QPushButton#btnSplitline {
	background-color: rgb(55,55,55);
	color: white;
}

QPushButton#btnSplitline:disabled {
	background-color: rgb(55,55,55);
    color: white;
}

QPushButton#btnSplitline:default {
    background-color: rgb(55,55,55);
    color: white;
}

QPushButton#btnPanel{
	background-color: transparent;
	color: white;
}

QPushButton#btnPanel:disabled {
	background-color: transparent;
	color: white;
}

QPushButton#btnPanel:default {
    background-color: transparent;
	color: white;
}

QPushButton#btnPanel:hover {
    background-color: transparent;
	color: white;
}

QPushButton#btnPanel:pressed {
    background-color: transparent;
	color: white;
}

QPushButton#btnPanel:checked {
	background-color: transparent;
	color: white;
}


*[menubutton="true"] {
    background-color: rgb(62, 62, 66);
    color: white;
}

*[menubutton="true"]:hover {
    background-color: rgb(84, 166, 247);
    color: white;
}

*[menubutton="true"]:pressed {
    background-color: rgb(0, 111, 185);
    color: white;
}

*[AniCtrl="true"] {
    background-color: rgb(37, 37, 38);
    padding: 0px;
    min-width: 18;
    max-width: 18;
}

*[AniCtrl="true"]:hover {
    background-color: rgb(62, 62, 66);
    padding: 0px;
    min-width: 18;
    max-width: 18;
}

*[AniCtrl="true"]:pressed {
    background-color: rgb(0, 111, 185);
    padding: 0px;
    min-width: 18;
    max-width: 18;
}

QCalendarView {
    color: grey;
}

QMenu {
    background-color: rgb(30, 30, 32);
    border: 1px solid rgb(60, 60, 60);
}

QMenu::item:disabled {
    color: gray;
}

QToolBar {
    /*background-color: rgb(45, 45, 48);*/
	background-color:transparent;
    border: 1px solid rgb(30, 30, 32);
	/*background-color: rgb(55, 55, 55);*/
}

QToolBar::item {
    padding: 5px 10px 5px 10px;
    background: transparent;
	/*background-color: rgb(35, 35, 35);*/
}

QToolBar::action {
    padding: 5px 10px 5px 10px;
    /*background: transparent;*/
	background-color: rgb(30, 30, 32);
}

QToolBar:handle {
    background: rgb(55, 55, 55);
    border-left: 1px dotted yellow;
    color: rgb(70, 70, 74);
}

QToolBar::separator {
    /*background-color: rgb(70, 70, 74);*/
	background-color: rgb(55, 55, 55);
    width: 1px;
    height: 1px;
}

QAction {
	background-color: rgb(55, 55, 55);
}

QScrollBar {
    background-color: rgb(62, 62, 66);
}

QScrollBar::handle:vertical {
    background-color: rgb(104, 104, 104);
    margin: 15px 3px 15px 3px;
    min-height: 10px;
}

QScrollBar::handle:horizontal {
    background-color: rgb(104, 104, 104);
    margin: 3px 15px 3px 15px;
    min-width: 10px;
}

QScrollBar::handle:hover {
    background-color: rgb(158, 158, 158);
}

QScrollBar::handle:pressed {
    background-color: rgb(239, 235, 239);
}

QScrollBar::handle:disabled {
    background-color: rgb(62, 62, 66);
}

QScrollBar::sub-line, QScrollBar::add-line {
    border: 1px solid rgb(62, 62, 66);
}

QScrollBar::up-arrow:vertical {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/up-arror.png);
}

QScrollBar::up-arrow:vertical:hover {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/up-arrow-h.png);
}

QScrollBar::up-arrow:vertical:pressed {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/up-arrow-p.png);
}
QScrollBar::down-arrow:vertical {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/down-arror.png);
}

QScrollBar::down-arrow:vertical:hover {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/down-arrow-h.png);
}

QScrollBar::down-arrow:vertical:pressed {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/down-arrow-p.png);
}

QScrollBar:left-arrow:horizontal {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/left-arror.png);
}

QScrollBar:left-arrow:horizontal:hover {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/left-arrow-h.png);
}

QScrollBar:left-arrow:horizontal:pressed {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/left-arrow-p.png);
}

QScrollBar::right-arrow:horizontal {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/right-arror.png);
}

QScrollBar::right-arrow:horizontal:hover {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/right-arrow-h.png);
}

QScrollBar::right-arrow:horizontal:pressed {
    subcontrol-origin: margin;
    border-image: url(:/Ampctrl/Recources/image/right-arrow-p.png);
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QTableView {
	background-color: rgb(50, 50, 50);
}

QToolTip {
    background-color: rgb(30, 30, 30);
    border: 1px solid rgb(45, 45, 48);
}

QSlider#m_pSlider {
    background-color: rgb(62, 62, 66);
}

QSlider::groove:horizontal#m_pSlider {
    height: 4px;
    background-color: rgb(30, 30, 30);
    border: 1px solid rgb(20, 20, 20);
    border-radius: 2px;
}

QSlider::handle:horizontal#m_pSlider {
    /*background-color: rgb(45, 45, 48);*/
    background-color: rgb(0, 111, 185);
    border: 1px solid rgb(20, 20, 20);
    width: 8px;
    margin: -2px 0;
    /*border-radius: 3px;*/
}

QComboBox {
    border: 1px solid rgb(62, 62, 66);
    background-color: rgb(37, 37, 38);
}

QComboBox QAbstractItemView {
    background-color: rgb(30, 30, 30);
    border: 1px solid rgb(62, 62, 66);
}

QComboBox:hover {
    border: 1px solid rgb(0, 111, 185);
}

QComboBox::drop-down {
    border: 1px solid rgb(62, 62, 66);  
}

QComboBox::down-arrow {
    image: url(:/Ampctrl/Recources/image/down-arror.png);
}

QComboBox::down-arrow:hover {
    image: url(:/Ampctrl/Recources/image/down-arrow-h.png);
}

QComboBox::down-arrow:pressed {
    image: url(:/Ampctrl/Recources/image/down-arrow-p.png);
}

QComboBox:disabled {
   background-color:rgb(70,70,70);
   color:rgb(150,150,150);
}

QSpinBox, QDoubleSpinBox {
    border: 1px solid rgb(62, 62, 66);
    background-color: rgb(37, 37, 38);
}

QSpinBox:hover, QDoubleSpinBox:hover {
    border: 1px solid rgb(0, 111, 185);
    background-color: rgb(63, 63, 70);
}

QSpinBox:disabled, QDoubleSpinBox:disabled{
    background-color: gray;
    border: 1px solid gray;
}


QDoubleSpinBox::up-arrow, QSpinBox::up-arrow {
    image: url(:/Ampctrl/Recources/image/add-arrow.png);
}

QDoubleSpinBox::up-arrow:hover, QSpinBox::up-arrow:hover {
    image: url(:/Ampctrl/Recources/image/add-arrow-h.png);
}

QDoubleSpinBox::up-arrow:pressed, QSpinBox::up-arrow:pressed {
    image: url(:/Ampctrl/Recources/image/add-arrow-p.png);
}


QSpinBox::up-button, QDoubleSpinBox::up-button {
    border: 1px solid rgb(62, 62, 66);
}


QDoubleSpinBox::down-arrow, QSpinBox::down-arrow {
    image: url(:/Ampctrl/Recources/image/sub-arrow.png);
}

QDoubleSpinBox::down-arrow:hover, QSpinBox::down-arrow:hover {
    image: url(:/Ampctrl/Recources/image/sub-arrow-h.png);
}

QDoubleSpinBox::down-arrow:pressed, QSpinBox::down-arrow:pressed {
    image: url(:/Ampctrl/Recources/image/sub-arrow-p.png);
}


QSpinBox::down-button, QDoubleSpinBox::down-button {
    border: 1px solid rgb(62, 62, 66);
}

